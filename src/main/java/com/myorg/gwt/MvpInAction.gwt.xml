<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='mvpinaction'>
	<inherits name='com.google.gwt.user.User' />
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place"/>
    
	<entry-point class='com.myorg.gwt.client.MvpInActionEntryPoint' />
    
    <replace-with class="com.myorg.gwt.client.ClientFactoryImpl">
        <when-type-is class="com.myorg.gwt.client.ClientFactory" />
    </replace-with>
    
	<source path='client' />
</module>
