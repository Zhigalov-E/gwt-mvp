<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:gwt='urn:import:com.google.gwt.user.client.ui'>
    <ui:with type="com.myorg.gwt.main.client.mvp.view.css.MainResources" field="css"/>
    <ui:with type="com.myorg.gwt.common.client.i18n.AppMessages" field="i18n"/>

    <gwt:HTMLPanel>
        <div class="{css.style.greetingBox}">
            <br></br>
            <br></br>
            <br></br>
            <div style='display:center'>
                <gwt:Label ui:field="userGreeting" text=""/>
            </div>
            <br></br>
            <br></br>
            <br></br>
            <gwt:Anchor ui:field="logOut" text="{i18n.logout}" href="#login:"/>
        </div>
        <gwt:VerticalPanel styleName="{css.style.fileLoadForm}">
            <gwt:FormPanel ui:field="form"  method="POST" encoding="multipart/form-data" action="/fileProcessor">
                <gwt:HorizontalPanel>
                    <gwt:FileUpload ui:field="uploadField" name="file"/>
                    <gwt:SubmitButton ui:field="uploadButton">Upload</gwt:SubmitButton>
                    <gwt:Button ui:field="clearButton">Clear</gwt:Button>
                </gwt:HorizontalPanel>
            </gwt:FormPanel>

            <gwt:TextArea ui:field="clientData" styleName="{css.style.clientData}"></gwt:TextArea>
        </gwt:VerticalPanel>

    </gwt:HTMLPanel>
</ui:UiBinder>
 