<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='Login'>
    <inherits name="com.myorg.gwt.common.Common"/>

    <entry-point class="com.myorg.gwt.login.client.LoginEntryPoint"/>

    <replace-with class="com.myorg.gwt.login.client.ClientFactoryImpl">
        <when-type-is class="com.myorg.gwt.login.client.ClientFactory"/>
    </replace-with>

    <source path='client'/>
</module>
